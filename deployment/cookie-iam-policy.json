{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowCookieOperations",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::${COOKIE_S3_BUCKET}/cookies/*"
      ]
    }
  ]
}